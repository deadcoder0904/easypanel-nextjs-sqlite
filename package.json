{
  "name": "drizzle-t3-oss-env",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "drizzle-kit push:sqlite --config drizzle.config.ts",
    "db:generate": "drizzle-kit generate:sqlite --config drizzle.config.ts",
    "db:seed": "node --import tsx --env-file .env.development ./src/scripts/seed/insert.ts",
    "db:delete": "node --import tsx --env-file .env.development ./src/scripts/seed/delete.ts"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.9.2",
    "better-sqlite3": "^9.4.0",
    "drizzle-orm": "^0.29.3",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "std-env": "^3.7.0",
    "ulidx": "^2.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "drizzle-kit": "^0.20.14",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
